upstream car_managements_production_unicorn {
  server unix:/tmp/unicorn.car_managements_production_unicorn.sock fail_timeout=0;
}
server {
	listen 888;
	#server_name test.dc.ikcrm.com;

  root /dyne/wp_apps/car_managements_production/current/public;
  # individual nginx logs for this car_managements vhost
  access_log  /var/log/nginx/car_managements_production_access.log;
  error_log   /var/log/nginx/car_managements_production_error.log;

  location ^~ /assets/ {
    gzip_static on;
    expires max;
    add_header Cache-Control public;
  }

  try_files $uri/index.html $uri @unicorn;

  location @unicorn {
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header Host $http_host;
    proxy_redirect off;
    proxy_pass http://car_managements_production_unicorn;
  }

  error_page 500 502 503 504 /500.html;
  client_max_body_size 4G;
  keepalive_timeout 10;
}

